<?xml version="1.0" encoding="ASCII"?>
<swing:SwingModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:swing="http://www.modelum.es/swing"
    xsi:schemaLocation="http://www.modelum.es/swing ../metamodels/ecores/swing-metamodelo.ecore"
    name="model">
  <windows title="My first window">
    <contentPane
        background="//@colors.0"
        font="//@fonts.1">
      <components
          xsi:type="swing:Box"
          background="//@colors.1"
          font="//@fonts.1"/>
      <components
          xsi:type="swing:JPanel"
          level="1">
        <components
            xsi:type="swing:JLabel"
            background="//@colors.1"
            font="//@fonts.0"
            text="Login"/>
        <components
            xsi:type="swing:JTextField"
            background="//@colors.2"
            font="//@fonts.1"
            text="Lorem Ipsum"/>
        <components
            xsi:type="swing:JList"
            background="//@colors.2"
            font="//@fonts.2">
          <listModel
              elements="&quot;Element1&quot;, &quot;Element2&quot;, &quot;Element3&quot;"/>
        </components>
      </components>
      <components
          xsi:type="swing:JPanel"
          level="1">
        <components
            xsi:type="swing:JButton"
            background="//@colors.1"
            font="//@fonts.1"
            text="Samole button"/>
      </components>
    </contentPane>
    <menuBar>
      <menus
          text="File">
        <items
            text="Logout"/>
      </menus>
      <menus
          text="About">
        <items
            text="Help"/>
        <items
            xsi:type="swing:JMenu"
            text="Submenu">
          <items
              text="Submenu 1.1"/>
          <items
              text="Submenu 1.2"/>
        </items>
      </menus>
    </menuBar>
  </windows>
  <colors name="Blue"
      b="255"/>
  <colors name="Green"
      g="255"/>
  <colors name="Red"
      r="255"/>
  <colors name="Black"/>
  <colors name="White"
      r="255"
      g="255"
      b="255"/>
  <fonts name="HelveticaBold"
      family="Helvetica"
      size="16"
      style="Bold"/>
  <fonts name="HelveticaRegular"
      family="Helvetica"
      size="16"
      style="Regular"/>
  <fonts name="HelveticaItalic"
      family="Helvetica"
      size="16"
      style="Italic"/>
</swing:SwingModel>
